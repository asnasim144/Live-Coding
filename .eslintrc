{
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-undef": "warn",
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "eqeqeq": ["warn", "always"],
    "curly": ["warn", "all"],
    "no-eval": "warn",
    "no-implied-eval": "warn",
    // "indent": ["warn", 2],
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "space-before-function-paren": ["warn", "never"],
    "arrow-spacing": ["warn", { "before": true, "after": true }],
    "no-var": "warn",
    "prefer-const": "warn",
    "no-duplicate-imports": "warn"
  }
}
